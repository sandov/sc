colorscheme tender

set tabstop=4
set softtabstop=0 noexpandtab
set shiftwidth=4

set nocompatible
set number

set encoding=utf-8
set showmatch

syntax enable

set showcmd
set ruler
set laststatus=2

"avoid redundant mode indicator if using lightline:
set noshowmode 

if (has("termguicolors"))
 set termguicolors
endif


